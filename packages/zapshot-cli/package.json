{
  "name": "zapshot-cli",
  "version": "0.4.0",
  "description": "JavaScript benchmarking tool to help continuous performance optimization",
  "bin": {
    "zapshot": "./dist/cli.js"
  },
  "engines": {
    "node": ">=8.0"
  },
  "scripts": {
    "prepare": "npm run build",
    "build": "../../node_modules/.bin/babel src --out-dir dist",
    "test": "../../node_modules/.bin/jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/Leko/zapshot.git"
  },
  "keywords": [
    "benchmark",
    "diff",
    "compare",
    "performance",
    "universal",
    "isomorphic"
  ],
  "author": "Leko <leko.noor@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Leko/zapshot/issues"
  },
  "homepage": "https://github.com/Leko/zapshot#readme",
  "dependencies": {
    "@turf/center-median": "^6.0.1",
    "chalk": "^2.4.1",
    "debug": "^3.1.0",
    "glob": "^7.1.2",
    "lodash": "^4.17.10",
    "napajs": "^0.2.3",
    "zapshot": "*"
  },
  "babel": {
    "extends": "../../.babelrc"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/*.{js,jsx}"
    ]
  }
}
