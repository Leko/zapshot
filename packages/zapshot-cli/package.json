{
  "name": "zapshot-cli",
  "version": "0.2.1",
  "description":
    "JavaScript benchmarking tool to help continuous performance optimization",
  "bin": {
    "zapshot": "./dist/cli.js"
  },
  "files": ["README.md", "package.json", "dist", "src"],
  "engines": {
    "node": ">=8.0"
  },
  "scripts": {
    "prepare": "npm run build",
    "build": "../../node_modules/.bin/babel src --out-dir dist",
    "test": "../../node_modules/.bin/jest --coverage"
  },
  "dependencies": {
    "@turf/center-median": "^6.0.1",
    "chalk": "^2.4.1",
    "debug": "^3.1.0",
    "glob": "^7.1.2",
    "lodash": "^4.17.10",
    "yargs": "^13.3.0",
    "zapshot": "^0.2.1"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/Leko/zapshot.git"
  },
  "bugs": {
    "url": "https://github.com/Leko/zapshot/issues"
  },
  "homepage":
    "https://github.com/Leko/zapshot/tree/master/packages/zapshot-cli",
  "author": "Leko <leko.noor@gmail.com>",
  "license": "MIT",
  "keywords": [
    "benchmark",
    "diff",
    "compare",
    "performance",
    "universal",
    "isomorphic"
  ],
  "babel": {
    "extends": "../../.babelrc"
  },
  "jest": {
    "collectCoverageFrom": ["src/*.{js,jsx}"]
  }
}
