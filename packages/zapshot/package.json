    "sinon": "^7.2.3"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/Leko/zapshot.git"
  },
  "bugs": {
    "url": "https://github.com/Leko/zapshot/issues"
  },
  "homepage": "https://github.com/Leko/zapshot/tree/master/packages/zapshot",
  "author": "Leko <leko.noor@gmail.com>",
  "license": "MIT",
  "keywords": [
    "benchmark",
    "diff",
    "compare",
    "performance",
    "universal",
    "isomorphic"
  ],
  "babel": {
    "extends": "../../.babelrc"
  },
  "jest": {
