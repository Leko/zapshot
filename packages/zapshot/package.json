{
  "name": "zapshot",
  "version": "0.4.0",
  "description": "JavaScript benchmarking tool to help continuous performance optimization",
  "main": "dist/index.js",
  "engines": {
    "node": ">=8.0"
  },
  "scripts": {
    "prepare": "npm run build",
    "build": "../../node_modules/.bin/babel src --out-dir dist",
    "test": "../../node_modules/.bin/jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/Leko/zapshot.git"
  },
  "keywords": [
    "benchmark",
    "diff",
    "compare",
    "performance",
    "universal",
    "isomorphic"
  ],
  "author": "Leko <leko.noor@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Leko/zapshot/issues"
  },
  "homepage": "https://github.com/Leko/zapshot#readme",
  "babel": {
    "extends": "../../.babelrc"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/*.{js,jsx}"
    ]
  },
  "dependencies": {
    "chalk": "^2.4.1",
    "lodash": "^4.17.10"
  },
  "devDependencies": {
    "nothing-mock": "^1.0.2",
    "sinon": "^5.0.7"
  }
}
